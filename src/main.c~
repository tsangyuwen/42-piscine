#include "bsq.h"

int		main(void)
{
	char **c;

	*c = malloc(20);
	c[0] = malloc(20);
	c[1] = malloc(20);
	c[2] = malloc(20);
	c[3] = malloc(20);
	c[4] = malloc(20);
	c[5] = malloc(20);
	c[6] = malloc(20);
	c[7] = malloc(20);
	c[8] = malloc(20);
	c[9] = malloc(20);
	c[10] = malloc(20);

	c[0] = "...........................\0";
	c[1] = "....o......................\0";
	c[2] = "............o..............\0";
	c[3] = "...........................\0";
	c[4] = "....o......................\0";
	c[5] = "...............o...........\0";
	c[6] = "...........................\0";
	c[7] = "......o..............o.....\0";
	c[8] = "..o.......o................\0";

	int k = 0;
	int j = 0;
	while (c[k] != 0)
	{
		j = 0;
		while (c[k][j] != '\0')
		{
			printf("%c", c[k][j]);
			j++;
		}
		printf("\n");
		k++;
	}
	return (0);
}
